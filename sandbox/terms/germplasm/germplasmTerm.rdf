    <skos:altLabel xml:lang="en" rdf:datatype="&germplasmTerm;mcpdTerm">biological status of sample</skos:altLabel>
    <dcterms:source rdf:resource="http://www.bioversityinternational.org/uploads/tx_news/1526.pdf"/>
    <skos:narrowMatch rdf:resource="dwc:basisOfRecord"/>
    <skos:exactMatch rdf:resource="co:CO_010:0000022/"/>
    <skos:exactMatch rdf:resource="co:CO_020:0000020/"/>
    <rdfs:domain rdf:resource="&germplasmTerm;GermplasmAccession"/>
    <rdfs:range rdf:resource="&germplasmType;BiologicalStatusType"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasmTerm;storageCondition">
    <skos:prefLabel xml:lang="en">storage conditions</skos:prefLabel>
    <skos:definition xml:lang="en">Type of germplasm storage conditions.</skos:definition>
    <skos:example xml:lang="en">10; 11; 12; 13; 20; 30; 40; 99</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use a controlled vocabulary such as the code list defined by the MCPD term STORAGE. Refer to FAO/IPGRI Genebank Standards 1994 for details on storage type.</skos:note>
