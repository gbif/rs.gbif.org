    <dcterms:isReplacedBy rdf:resource="&germplasm;traitScale"/>
  </skos:Concept>

  <skos:Concept rdf:about="&dwcg;GermplasmTraitSource">
    <skos:prefLabel xml:lang="en">trait descriptor source</skos:prefLabel>
    <skos:definition xml:lang="en">Reference to a standard Germplasm Descriptor. For example the UPOV descriptor or the Bioversity Crop Descriptor identifier.</skos:definition>
    <skos:example xml:lang="en">UPOV TG/198/1 (april 2003)</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use a controlled vocabulary of source trait descriptors.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/GermplasmTraitSource"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;TraitDescriptor"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;traitSource"/>
    <skos:relatedMatch rdf:resource="dwc:measurementMethod"/>
  </skos:Concept>

  <skos:Concept rdf:about="&dwcg;GermplasmTraitRemarks">
    <skos:prefLabel xml:lang="en">trait remarks</skos:prefLabel>
    <skos:definition xml:lang="en">Remarks related to the trait measurement method.</skos:definition>
    <skos:example xml:lang="en">inflorescences after cutting; measured three days after heading date</skos:example>
    <skos:note xml:lang="en">Any general remark that helps interpret the trait measurement method.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/GermplasmTraitRemarks"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;TraitDescriptor"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;traitRemarks"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&dwcg;epgrisTerm">TRAIT_REMARK</skos:altLabel>
    <skos:altLabel xml:lang="en">Measurement remarks</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <skos:relatedMatch rdf:resource="dwc:measurementRemarks"/>
  </skos:Concept>


  <skos:Concept rdf:about="&dwcg;GermplasmExperimentID">
    <skos:prefLabel xml:lang="en">trait experiment ID</skos:prefLabel>
    <skos:definition xml:lang="en">Persistent identifier used to identify a trait measurement experiment.</skos:definition>
    <skos:example xml:lang="en">URI, DOI, LSID or similar</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use a persistent identifier for trait measurement experiments.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/GermplasmExperimentID"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;TraitExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;traitExperimentID"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
  </skos:Concept>

  <skos:Concept rdf:about="&dwcg;GermplasmExperimentIdentifier">
    <skos:prefLabel xml:lang="en">trait experiment identifier</skos:prefLabel>
    <skos:definition xml:lang="en">Unique number in the dataset for the experiment.</skos:definition>
    <skos:example xml:lang="en">Local identifier for the experiment or project activity.</skos:example>
    <skos:note xml:lang="en">This number should be unique and persistent for the data provider. Recommended best practice is to use the GermplasmExperimentID term whenever possible.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/GermplasmExperimentIdentifier"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;TraitExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;traitExperimentIdentifier"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&dwcg;epgrisTerm">EXPERIMENT_NUMBER</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment number</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
  </skos:Concept>

  <skos:Concept rdf:about="&dwcg;GermplasmExperimentRemarks">
    <skos:prefLabel xml:lang="en">trait experiment remarks</skos:prefLabel>
    <skos:definition xml:lang="en">Information relevant for the interpretation of the scores in the experiment.</skos:definition>
    <skos:example xml:lang="en">Experimental design, location, experimentor, weather, etc.</skos:example>
    <skos:note xml:lang="en">General remarks related to the experimental conditions or experiment design.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/GermplasmExperimentRemarks"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;TraitExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;traitExperimentRemarks"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&dwcg;epgrisTerm">EXPERIMENT_DESCRIPTION</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment description</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <skos:relatedMatch rdf:resource="dwc:measurementRemarks"/>
  </skos:Concept>

  <skos:Concept rdf:about="&dwcg;GermplasmExperimentYear">
    <skos:prefLabel xml:lang="en">trait experiment year</skos:prefLabel>
    <skos:definition xml:lang="en">The year when the trait experiment was conducted.</skos:definition>
    <skos:example xml:lang="en">2010</skos:example>
    <skos:note xml:lang="en">For the more detailed experiment date, best practice is to use the dwc:measurementDeterminedDate term.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/GermplasmExperimentYear"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;TraitExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;traitExperimentYear"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&dwcg;epgrisTerm">EXPERIMENT_YEAR</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment year</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <skos:relatedMatch rdf:resource="dwc:measurementDeterminedDate"/>
    <skos:relatedMatch rdf:resource="dwc:year"/>
  </skos:Concept>

  <skos:Concept rdf:about="&dwcg;GermplasmExperimentReport">
    <skos:prefLabel xml:lang="en">trait experiment report</skos:prefLabel>
    <skos:definition xml:lang="en">A reference to the report of the experiment, either supplied as the file name or the URL of the report.</skos:definition>
    <skos:example xml:lang="en">URL or reference citation</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to provide the URL to the report.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/GermplasmExperimentReport"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;TraitExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;traitExperimentReport"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&dwcg;epgrisTerm">EXPERIMENT_REPORT</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment report</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
  </skos:Concept>

  <skos:Concept rdf:about="&dwcg;MeasurementByInstituteCode">
    <skos:prefLabel xml:lang="en">Trait measurement by institute</skos:prefLabel>
    <skos:definition xml:lang="en">Trait observation determined by institute code, INSTCODE. FAO WIEWS institute code for the institute performing the observation/measurement.</skos:definition>
    <skos:example xml:lang="en">DEU146; NLD037; SWE054; urn:lsid:biocol.org:col:35293</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use institute codes from the FAO WIEWS information system, "http://apps3.fao.org/wiews/institute_query.htm?i_l=EN" or the Biodiversity Collections Index (BCI), "http://www.biodiversitycollectionsindex.org/".</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/MeasurementByInstituteCode"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;TraitMeasurement"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;traitMeasurementByInstituteID"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <skos:relatedMatch rdf:resource="dwc:measurementDeterminedBy"/>
  </skos:Concept>

  <skos:Concept rdf:about="&dwcg;MeasurementGrowthStage">
    <skos:prefLabel xml:lang="en">trait measurement at growth stage</skos:prefLabel>
    <skos:definition xml:lang="en">The growth stage of the plant when the trait observation or measurement was made.</skos:definition>
    <skos:example xml:lang="en">flowering; heading; ripening; etc.</skos:example>
    <skos:note xml:lang="en">A more general modifier of the TDWG SDD style could be applied here and include modifiers such as part of the plant where the observation was made (eg. leaf, stem, seeds).</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/BiologicalStatusOfSampleCode"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;TraitMeasurement"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;traitMeasurementGrowthStage"/>
  </skos:Concept>




<!-- ### Datatype declaration, Internal annotation properties ###
 Some internal annotation properties to provide the type of identifiers and alternative labels (skos:altLabel)
 There are different types of identifiers and declaring a datatype to keep them appart might perhaps be useful? 
 Examples how to declare datatypes for WIEWS and BCI identifiers:
  <dcterms:identifier rdf:datatype="&dwcg;bioColID">http://biocol.org/urn:lsid:biocol.org:col:35293</dcterms:identifier>
  <dcterms:identifier rdf:datatype="&dwcg;wiewsInstituteID">SWE054</dcterms:identifier>
  <skos:altLabel xml:lang="en" rdf:datatype="&dwcg;mcpdTerm">DONORNUMB</skos:altLabel>
-->

  <rdfs:Datatype rdf:about="&dwcg;wiewsInstituteID">
    <skos:prefLabel xml:lang="en">FAO WIEWS Institute Code</skos:prefLabel>
    <skos:definition xml:lang="en">A three letter country ISO code and a three (old) or four digit number.</skos:definition>
    <skos:example xml:lang="en">ITA247; ITA406; NOR051; SWE002; SWE054</skos:example>
    <skos:note xml:lang="en">The FAO WIEWS Institute Codes are available at http://apps3.fao.org/wiews/institute_query.htm</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/wiewsInstituteID"/>
    <dcterms:issued>2012-03-27</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;wiewsInstituteID"/>
    <skos:closeMatch rdf:resource="dcterms:identifier"/>
    <skos:relatedMatch rdf:resource="http://apps3.fao.org/wiews/wiews.jsp"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdfs:Datatype>

  <rdfs:Datatype rdf:about="&dwcg;bioColID">
    <skos:prefLabel xml:lang="en">Biodiversity Collections Index ID</skos:prefLabel>
    <skos:definition xml:lang="en">An index of biodiversity collections, providing persistent identifiers based on LSID URIs.</skos:definition>
    <skos:example xml:lang="en">http://biocol.org/urn:lsid:biocol.org:col:35293</skos:example>
    <skos:note xml:lang="en">The Biodiversity Collections Index is available at http://biocol.org/</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/bioColID"/>
    <dcterms:issued>2012-03-27</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;bioCollID"/>
    <skos:closeMatch rdf:resource="dcterms:identifier"/>
    <skos:relatedMatch rdf:resource="http://biocol.org/"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdfs:Datatype>

  <rdfs:Datatype rdf:about="&dwcg;mcpdTerm">
    <skos:prefLabel xml:lang="en">Equivalent MCPD term</skos:prefLabel>
    <skos:definition xml:lang="en">Equivalent term from the MultiCrop Passport Descriptor list.</skos:definition>
    <skos:example xml:lang="en">INSTCODE; ACCENUMB; COLLNUMB; etc.</skos:example>
    <skos:note xml:lang="en">The MCPD terms are available at "http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"</skos:note>
