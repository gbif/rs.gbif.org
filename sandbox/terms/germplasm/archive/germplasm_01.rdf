    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
  </skos:Concept>


  <skos:Concept rdf:about="&dwcg;SafetyDuplicationID">
    <skos:prefLabel xml:lang="en">safety duplication ID</skos:prefLabel>
    <skos:definition xml:lang="en">Persistent identifier for the genebank accession in the safety duplication storage. A globally unique identifier (GUID), persistent identifier (PI) such as a URI, DOI, LSID or similar is preferred.</skos:definition>
    <skos:example xml:lang="en">URI, DOI, LSID or similar.</skos:example>
    <skos:note xml:lang="en">Could also be an unique and stable identifier specific to the safety storage collection.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/SafetyDuplicationID"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;SafetyDuplication"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;safetyDuplicationID"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <skos:narrower rdf:resource="dwcg:germplasmID"/>
    <skos:relatedMatch rdf:resource="dwc:otherCatalogNumbers"/>
  </skos:Concept>

