    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/SafetyDuplicationInstituteCode"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
