  </skos:Concept>

  <skos:Concept rdf:about="&dwcg;BiologicalStatusOfSample">
    <skos:prefLabel xml:lang="en">biological status</skos:prefLabel>
    <skos:definition xml:lang="en">The biological status for a germplasm accession describes the cultivation status and distinguish wild and primitive cultivars from modern cultivars and breeding lines (material under development) (MCPD term 20).</skos:definition>
    <skos:example xml:lang="en">Wild; Crop wild relative (CWR); Primitive; Traditional cultivar; Landrace; Breeding line; Genetic stock; Modern cultivar; 100; 110; 120; 200; 300; 400; 410; 411; 412; 413; 414; 415; 420; 500; 999</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to follow a controlled vocabulary, such as the code list defined by the MCPD term 20, SAMPSTAT.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/BiologicalStatusOfSample"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;GermplasmAccession"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;biologicalStatus"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&dwcg;mcpdTerm">SAMPSTAT</skos:altLabel>
    <skos:altLabel xml:lang="en">biological status of sample</skos:altLabel>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
    <skos:narrowMatch rdf:resource="dwc:basisOfRecord"/>
  </skos:Concept>

  <skos:Concept rdf:about="&dwcg;BiologicalStatusOfSampleCode">
    <skos:prefLabel xml:lang="en">biological status code</skos:prefLabel>
    <skos:definition xml:lang="en">The biological status codes for germplasm accessions describes the cultivation status and distinguish wild and primitive cultivars from modern cultivars and breeding lines (material under development). A numeric code system defined by the MultiCrop Passport Descriptors (MCPD term 20).</skos:definition>
    <skos:example xml:lang="en">100; 110; 120; 200; 300; 400; 410; 411; 412; 413; 414; 415; 420; 500; 999</skos:example>
    <skos:note xml:lang="en">Please follow the controlled vocabulary defined by the MCPD term 20, SAMPSTAT.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/BiologicalStatusOfSampleCode"/>
    <dcterms:issued>2009-08-24</dcterms:issued>
    <dcterms:modified>2012-03-27</dcterms:modified>
    <vann:termGroup rdf:resource="&dwcg;GermplasmAccession"/>
    <rdfs:isDefinedBy rdf:resource="&dwcg;"/>
    <skos:inScheme rdf:resource="&dwcg;"/>
    <vs:term_status>archaic</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;biologicalStatus"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&dwcg;mcpdTerm">SAMPSTAT</skos:altLabel>
    <skos:altLabel xml:lang="en">biological status of sample</skos:altLabel>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
