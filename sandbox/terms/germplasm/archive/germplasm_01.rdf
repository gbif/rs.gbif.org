  </rdfs:Datatype>

  <rdfs:Datatype rdf:about="&dwcg;euriscoTerm">
    <skos:prefLabel xml:lang="en">Equivalent EURISCO term</skos:prefLabel>
    <skos:definition xml:lang="en">Equivalent term from the European Germplasm Search Catalog, amendment to the MCPD.</skos:definition>
    <skos:example xml:lang="en">NICODE; COLLDESCR; BREDDESCR; DONORDESCR; DUPLDESCR; ACCEURL</skos:example>
