    <skos:example xml:lang="en">DATASET; UPLOADERCODE; DATASET_REMARK; EXPERIMENT; EXPERIMENT_NUMBER; EXPERIMENT_DESCRIPTION; EXPERIMENT_YEAR; EXPERIMENT_LONGITUDE; EXPERIMENT_LATITUDE; EXPERIMENT_REPORT; TRAIT; TRAIT_NUMBER; TRAIT_NAME; TRAIT_REMARK; TRAIT_METHOD; GENOTYPE; GENOTYPE_NUMBER; GENOTYPE_NICODE; GENOTYPE_INSTCODE; GENOTYPE_ACCENUMB; GENOTYPE_GENUS; SCORE</skos:example>
