<?xml version="1.0" encoding="UTF-8"?>

<!-- Discussion document: version 2012-08-03.
  Dag Endresen, Global Biodiversity Information Facility (GBIF)
 This RDF Vocabulary includes terminology for Natural Collections Decriptions (NCD)
 developed by the TDWG NCD task group. 
 Terms and term identifiers used by this resource was extracted from the 
 NCD version 0.9 and the TDWG LSID ontology.
-->

<!--
	August 2012, Dag Endresen (GBIF)
	Some errors in the NCD 0.9 and TDWG Ontology was corrected.
	ERROR :: all Development Status Type terms was declared as owl:DevelopmentStatusTypeTerm - owl prefix was removed
	ERROR :: all Conservation Status Type terms was declared as owl:ConservationStatusTypeTerm - owl prefix was removed
	Further information declared using skos:editorialNote and marked as "WARNING:" or "NOTE:".
 -->

<!DOCTYPE rdf:RDF [
  <!ENTITY tdwgNcd      "http://rs.tdwg.org/ontology/voc/Collection#">
  <!ENTITY tdwgNcdType  "http://rs.tdwg.org/ontology/voc/CollectionType#">
  <!ENTITY tdwgVoc      "http://rs.tdwg.org/ontology/voc/">
  <!ENTITY ncdConcept   "http://rs.gbif.org/terms/ncd/ncdConcept#">
  <!ENTITY ncdType      "http://rs.gbif.org/terms/ncd/ncdType#">
  <!ENTITY contact      "http://rs.gbif.org/terms/ncd/ncdContact#">
  <!ENTITY history      "http://rs.gbif.org/terms/ncd/archive/">
]>

<rdf:RDF
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:skos="http://www.w3.org/2004/02/skos/core#"
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
  xmlns:vann="http://purl.org/vocab/vann/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:dcmitype="http://purl.org/dc/dcmitype/"
  xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
  xmlns:dwc="http://rs.tdwg.org/dwc/terms/"
  xmlns:ncd="&tdwgNcd;"
  xmlns:ncdConcept="&ncdConcept;"
  xmlns:ncdType="&ncdType;"
  xml:base="&ncdConcept;"
>


<!-- ### Vocabulary of term concepts (skos:ConceptScheme) ### -->

  <skos:ConceptScheme rdf:about="&ncdConcept;">
    <skos:prefLabel xml:lang="en">Natural Collections Description (NCD)</skos:prefLabel>
    <skos:definition xml:lang="en">Natural Collections Description (NCD) is a data standard for describing collections of natural history materials at the collection level; one NCD record describes one entire collection.</skos:definition>
    <dcterms:created>2007-03-05</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dc:creator rdf:resource="&contact;GBIF"/>
    <dcterms:contributor rdf:resource="&contact;TDWG_NCD_IG"/>
    <dcterms:contributor rdf:resource="&contact;MarkusDoring"/>
    <dcterms:contributor rdf:resource="&contact;ConstanceRinaldo"/>
    <dcterms:contributor rdf:resource="&contact;DagEndresen"/>
    <dcterms:publisher rdf:resource="&contact;GBIF"/>
    <dcterms:license rdf:resource="http://creativecommons.org/publicdomain/zero/1.0/"/> <!-- NB! Licence rights...? -->
    <dcterms:rights xml:lang="en">Creative Commons Public Domain license; see http://creativecommons.org/publicdomain/zero/1.0/. You are free to copy, distribute and transmit the work; to remix/adapt the work (e.g. to import the ontology and create specializations of its elements), all without asking permissions. If you wish to provide attribution you may include the statement: "This work is based on the Darwin Core extension for genebanks".</dcterms:rights>
    <dcterms:accessRights>public</dcterms:accessRights>
    <vann:preferredNamespaceUri>&tdwgNcd;</vann:preferredNamespaceUri>
    <vann:preferredNamespacePrefix>ncd</vann:preferredNamespacePrefix>
    <foaf:homepage rdf:resource="http://www.tdwg.org/activities/ncd/"/>
    <dcterms:bibliographicCitation>TDWG NCD Interest Group (2009). Natural Collections Descriptions (NCD): A data standard for exchanging data describing natural history collections (Cover Page). Biodiversity Information Standards (TDWG). Available at http://www.tdwg.org/standards/312/, verified 20 June 2012.</dcterms:bibliographicCitation>
    <dcterms:bibliographicCitation>Thomson N, Hyam R, Rinaldo C, Butler C, Holland D, Mathe B, Waibel B, Addink W, Altenburg R, and Döring M (2009). Natural Collections Description (NCD): A data standard for exchanging data describing natural history collections. Biodiversity Information Standards (TDWG). Available at http://www.tdwg.org/standards/312/, verified 20 June 2012.</dcterms:bibliographicCitation>
    <skos:note xml:lang="en">Notice that the base URI for the NCD vocabulary is "http://rs.tdwg.org/ontology/voc/Collection". Please make sure that you are only using element identifiers in this namespace, e.g. "http://rs.tdwg.org/ontology/voc/CollectionTERM". There may be copies of the vocabulary file on the Web which can be retrieved from other locations, but those locations should not be used as the basis of identifiers.</skos:note>
    <skos:editorialNote>Terms and definitions for this vocabulary of terms was extracted from the documentation NCD version 0.9 and from the TDWG Ontology.</skos:editorialNote>
    <rdfs:seeAlso rdf:resource="http://kos.gbif.org/wiki/NCD"/>
    <rdfs:seeAlso rdf:resource="http://www.tdwg.org/standards/312/"/>
    <rdfs:seeAlso rdf:resource="http://www.tdwg.org/activities/ncd/"/>
    <owl:versionInfo xml:lang="en">Draft version, not yet published</owl:versionInfo>
    <dcterms:hasVersion rdf:resource="&history;20120803/ncdConcept.rdf"/> <!-- SKOS version for the set of NCD concepts -->
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
  </skos:ConceptScheme>


  <!-- NCD concepts are described by
   TermURI	
   skos:prefLabel	
   dc:description	
   vann:termGroup	
   rdf:type	
   rdfs:isDefinedBy	
   skos:inScheme	
   dcterms:modified	
   skos:example	
   skos:note	
   skos:editorialNote	
  -->

  <rdf:Description rdf:about="http://purl.org/dc/dcmitype/Dataset">
    <skos:prefLabel xml:lang="en">Dataset</skos:prefLabel>
    <dc:description xml:lang="en">Data encoded in a defined structure.</dc:description>
    <vann:termGroup rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <rdf:type rdf:resource="owl:Class"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/dcmitype/"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>Examples include lists, tables, and databases. A dataset may be useful for direct machine processing.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/source">
    <skos:prefLabel xml:lang="en">Record Source</skos:prefLabel>
    <dc:description xml:lang="en">Source of the record if not created by the author named in Author.</dc:description>
    <vann:termGroup rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <rdf:type rdf:resource="owl:ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>Test record transferred from NHM Collections Navigator. Originally authored in the NHM Unicorn LMS</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/creator">
    <skos:prefLabel xml:lang="en">Author</skos:prefLabel>
    <dc:description xml:lang="en">Person that created the record.</dc:description>
    <vann:termGroup rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <rdf:type rdf:resource="owl:ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>Susan Bennett</skos:example>
    <skos:note>mandatory</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/2001/vcard-rdf/3.0#Orgname">
    <skos:prefLabel xml:lang="en">Corporate Affiliation</skos:prefLabel>
    <dc:description xml:lang="en">Organisational affiliation of the author.</dc:description>
    <vann:termGroup rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2001/vcard-rdf/3.0#"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>Natural History Museum, London</skos:example>
    <skos:note>repeatable, text in local language</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/dc/terms/created">
    <skos:prefLabel xml:lang="en">Record Created Date</skos:prefLabel>
    <dc:description xml:lang="en">Date of record creation.</dc:description>
    <vann:termGroup rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>2002-09-09</skos:example>
    <skos:note>mandatory</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/contributor">
    <skos:prefLabel xml:lang="en">Editor</skos:prefLabel>
    <dc:description xml:lang="en">Person that last edited the record.</dc:description>
    <vann:termGroup rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>Neil Thomson</skos:example>
    <skos:note>mandatory</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/dc/terms/modified">
    <skos:prefLabel xml:lang="en">Record Edited Date</skos:prefLabel>
    <dc:description xml:lang="en">Date the record was last edited.</dc:description>
    <vann:termGroup rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>2008-07-15</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/rights">
    <skos:prefLabel xml:lang="en">Record Rights</skos:prefLabel>
    <dc:description xml:lang="en">IPR statement about the record.</dc:description>
    <vann:termGroup rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>© 1994 - 2002 The Natural History Museum (London)</skos:example>
    <skos:note>text in local language</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/2001/vcard-rdf/3.0#NOTE">
    <skos:prefLabel xml:lang="en">Notes</skos:prefLabel>
    <dc:description xml:lang="en">Notes</dc:description>
    <vann:termGroup rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2001/vcard-rdf/3.0#"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example></skos:example>
    <skos:note>text in local language</skos:note>
    <skos:editorialNote>WARNING: NCD 0.9 documentation uses term ID vcard:Note, vCard uses vcard:NOTE</skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://rs.tdwg.org/ontology/voc/Collection#Collection">
    <skos:prefLabel xml:lang="en">Collection</skos:prefLabel>
    <dc:description xml:lang="en">A group of specimens or other objects about natural history. This class represents actual collections. For derived collections please use the DerivedCollection subclass.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="owl:Class"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/Collection"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example></skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://rs.tdwg.org/ontology/voc/Collection#DerivedCollection">
    <skos:prefLabel xml:lang="en">Derived Collection</skos:prefLabel>
    <dc:description xml:lang="en">A "derived" collection record. The record has been derived from a query on an item-level database e.g. all items from Australia.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="owl:Class"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/Collection"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example></skos:example>
    <skos:note></skos:note>
    <skos:editorialNote>WARNING: NCD 0.9 use ncd:derivedCollection, TDWG ontology: ncd:DerivedCollection</skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://rs.tdwg.org/ontology/voc/Collection#collectionId">
    <skos:prefLabel xml:lang="en">Collection Identifier</skos:prefLabel>
    <dc:description xml:lang="en">The URI (LSID or URL) of the collection. In RDF, used as URI of the collection resource.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="owl:ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/Collection"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>LSID or URL of the RDF:about record</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://rs.tdwg.org/ontology/voc/Collection#alternativeId">
    <skos:prefLabel xml:lang="en">Alternative Identifier</skos:prefLabel>
    <dc:description xml:lang="en">Alternative identifier for the collection with an indication of the source e.g. ISCW.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="owl:ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/Collection"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>Museum Number: uls-a352878</skos:example>
    <skos:note>repeatable</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://rs.tdwg.org/ontology/voc/Collection#isPartOfCollection">
    <skos:prefLabel xml:lang="en">Parent Collection Identifier</skos:prefLabel>
    <dc:description xml:lang="en">Identifier for the parent collection for this sub-collection. Enables a hierarchy of collections and sub collections to be built.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="owl:ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/Collection"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example></skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://rs.tdwg.org/ontology/voc/Collection#acronymOrCoden">
    <skos:prefLabel xml:lang="en">Acronym or Coden</skos:prefLabel>
    <dc:description xml:lang="en">Acronym, coden or initialism by which the collection is generally known indicating the source, e.g. Index Herbariorum.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="owl:DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/Collection"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example></skos:example>
    <skos:note>repeatable</skos:note>
    <skos:editorialNote>WARNING: this term is missing in the TDWG ontology.</skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/title">
    <skos:prefLabel xml:lang="en">Collection Name</skos:prefLabel>
    <dc:description xml:lang="en">Official name of the Collection in the local language.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>Manuscript Collection of Hugh Whistler (1889-1943)</skos:example>
    <skos:note>mandatory, repeatable, text in local language</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/dc/terms/alternative">
    <skos:prefLabel xml:lang="en">Alternative Name</skos:prefLabel>
    <dc:description xml:lang="en">Alternative name for the collection. In English language where Collection name is not in English.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example></skos:example>
    <skos:note>repeatable, text in local language</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/2001/vcard-rdf/3.0#FN">
    <skos:prefLabel xml:lang="en">Associated Person</skos:prefLabel>
    <dc:description xml:lang="en">Name(s) of person(s) associated with the collection.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2001/vcard-rdf/3.0#"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>Whistler, Hugh, 1889-1943</skos:example>
    <skos:note>repeatable</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/description">
    <skos:prefLabel xml:lang="en">Description</skos:prefLabel>
    <dc:description xml:lang="en">Main description element may be anything from a few words to an essay, suitable for a general audience.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>The manuscript collection of Hugh Whistler (1889-1943) comprises notes on birds collected in 1926 on an expedition into the Shaksgam Valley and the Aghil Range (…)</skos:example>
    <skos:note>mandatory, text in local language</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://rs.tdwg.org/ontology/voc/Collection#descriptionForSpecialists">
    <skos:prefLabel xml:lang="en">Description for Specialists</skos:prefLabel>
    <dc:description xml:lang="en">Optional additional descriptive text using terms that are more suited to a specialist audience. Attribute indicates the language of the text.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="owl:DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/Collection"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>Hugh Whistler (1889-1943), F.Z.S., M.B.O.U., was born at Mablethorpe in Lincolnshire and (…)</skos:example>
    <skos:note>text in local language</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://rs.tdwg.org/ontology/voc/Collection#collectionExtent">
    <skos:prefLabel xml:lang="en">Extent</skos:prefLabel>
    <dc:description xml:lang="en">A free-text indication of the size or extent of the collection.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/Collection#Collection"/>
    <rdf:type rdf:resource="owl:DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/Collection"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdConcept.rdf"/>
    <skos:example>26 boxes of manuscript notes, 8 volumes, 2 typescripts, 1 manuscript, 2 notebooks, Letters, 2 newspaper cuttings and maps</skos:example>
    <skos:note>text in local language</skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </rdf:Description>

  <rdf:Description rdf:about="http://rs.tdwg.org/ontology/voc/Collection#collectionType">
    <skos:prefLabel xml:lang="en">Collection Type</skos:prefLabel>
    <dc:description xml:lang="en">Picklist keyword to indicate the typical content or unifying characteristic of the collection. Expected to contain an instance for the Collection Type Term vocabulary.</dc:description>
