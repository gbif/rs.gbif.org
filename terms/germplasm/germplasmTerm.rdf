    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>stable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;BiologicalStatusOfSample"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;mcpdTerm">SAMPSTAT</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;mcpdTerm">biological status of sample</skos:altLabel>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
    <skos:narrowMatch rdf:resource="dwc:basisOfRecord"/>
    <rdfs:domain rdf:resource="&germplasm;GermplasmAccession"/>
    <rdfs:range rdf:resource="&germplasm;BiologicalStatusType"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;storageCondition">
    <skos:prefLabel xml:lang="en">storage conditions</skos:prefLabel>
    <skos:definition xml:lang="en">Type of germplasm storage conditions.</skos:definition>
    <skos:example xml:lang="en">10; 11; 12; 13; 20; 30; 40; 99</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use a controlled vocabulary such as the code list defined by the MCPD term STORAGE. Refer to FAO/IPGRI Genebank Standards 1994 for details on storage type.</skos:note>
