    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;biologicalStatus">
    <skos:prefLabel xml:lang="en">biological status</skos:prefLabel>
    <skos:definition xml:lang="en">The biological status for a germplasm accession describes the cultivation status and distinguish wild and primitive cultivars from modern cultivars and breeding lines (material under development).</skos:definition>
    <skos:example xml:lang="en">Wild; Crop wild relative (CWR); Primitive; Traditional cultivar; Landrace; Breeding line; Genetic stock; Modern cultivar; 100; 110; 120; 200; 300; 400; 410; 411; 412; 413; 414; 415; 420; 500; 999</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to follow a controlled vocabulary, such as the code list defined by the MCPDv2 term 19, SAMPSTAT.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/GermplasmAccession"/>
    <skos:scopeNote rdf:resource="http://terms.gbif.org/wiki/germplasm:biologicalStatus"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dcterms:hasVersion rdf:resource="&history;germplasmTerm#biologicalStatus"/>
