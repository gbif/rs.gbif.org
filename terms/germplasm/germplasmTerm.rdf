    <skos:inScheme rdf:resource="&germplasm;"/>
    <skos:closeMatch rdf:resource="dcmitype:Dataset"/>
    <skos:relatedMatch rdf:resource="dcmitype:Collection"/>
    <rdfs:subClassOf rdf:resource="#anyClass"/>
    <skos:member rdf:resource="dwc:datasetID"/>
    <skos:member rdf:resource="dwc:datasetName"/>
    <skos:member rdf:resource="dwc:collectionID"/>
    <skos:member rdf:resource="dwc:collectionCode"/>
    <skos:member rdf:resource="dwc:institutionID"/>
    <skos:member rdf:resource="dwc:institutionCode"/>
  </skos:Collection>

