    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmTraitClass"/>
    <skos:relatedMatch rdf:resource="dwc:measurementType"/>
    <rdfs:domain rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;measurementMethodScale">
    <skos:prefLabel xml:lang="en">trait measurement scale</skos:prefLabel>
    <skos:definition xml:lang="en">Measurement scale used to score the measurement values.</skos:definition>
    <skos:example xml:lang="en">absolute; interval; nominal; ordinal; ratio; relative</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use a controlled vocabulary. The theory of scale types was developed by Stanley S. Stevens (1946) in Science 103(2684):677–680.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/MeasurementMethod"/>
    <skos:scopeNote rdf:resource="http://terms.gbif.org/wiki/germplasm:measurementMethodScale"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dcterms:hasVersion rdf:resource="&history;germplasmTerm#measurementMethodScale"/>
    <vann:termGroup rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmTraitScale"/>
    <rdfs:domain rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;measurementMethodSource">
    <skos:prefLabel xml:lang="en">trait descriptor source</skos:prefLabel>
    <skos:definition xml:lang="en">Reference to a standard Germplasm Descriptor. For example the UPOV descriptor or the Bioversity Crop Descriptor identifier.</skos:definition>
    <skos:example xml:lang="en">UPOV TG/198/1 (april 2003)</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use a controlled vocabulary of source trait descriptors.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/MeasurementMethod"/>
    <skos:scopeNote rdf:resource="http://terms.gbif.org/wiki/germplasm:measurementMethodSource"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dcterms:hasVersion rdf:resource="&history;germplasmTerm#measurementMethodSource"/>
    <vann:termGroup rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmTraitSource"/>
    <skos:relatedMatch rdf:resource="dwc:measurementMethod"/>
    <rdfs:domain rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;measurementMethodRemarks">
    <skos:prefLabel xml:lang="en">trait remarks</skos:prefLabel>
    <skos:definition xml:lang="en">Remarks related to the trait measurement method.</skos:definition>
    <skos:example xml:lang="en">inflorescences after cutting; measured three days after heading date</skos:example>
    <skos:note xml:lang="en">Any general remark that helps interpret the trait measurement method.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/MeasurementMethod"/>
    <skos:scopeNote rdf:resource="http://terms.gbif.org/wiki/germplasm:measurementMethodRemarks"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dcterms:hasVersion rdf:resource="&history;germplasmTerm#measurementMethodRemarks"/>
    <vann:termGroup rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmTraitRemarks"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">TRAIT_REMARK</skos:altLabel>
    <skos:altLabel xml:lang="en">Measurement remarks</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <skos:relatedMatch rdf:resource="dwc:measurementMethod"/>
    <rdfs:domain rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>


  <!-- Measurement experiment -->

  <skos:Concept rdf:about="&germplasm;measurementExperimentID">
    <skos:prefLabel xml:lang="en">measurement experiment ID</skos:prefLabel>
    <skos:definition xml:lang="en">Persistent identifier used to identify a trait measurement experiment.</skos:definition>
    <skos:example xml:lang="en">URI, DOI, LSID or similar</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use a persistent identifier for trait measurement experiments.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/MeasurementExperiment"/>
    <skos:scopeNote rdf:resource="http://terms.gbif.org/wiki/germplasm:measurementExperimentID"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dcterms:hasVersion rdf:resource="&history;germplasmTerm#measurementExperimentID"/>
    <vann:termGroup rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmExperimentID"/>
    <skos:relatedMatch rdf:resource="dcterms:identifier"/>
    <rdfs:domain rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;measurementExperimentIdentifier">
    <skos:prefLabel xml:lang="en">measurement experiment identifier</skos:prefLabel>
    <skos:definition xml:lang="en">Unique number in the dataset for the experiment.</skos:definition>
    <skos:example xml:lang="en">Local identifier for the experiment or project activity.</skos:example>
    <skos:note xml:lang="en">This number should be unique and persistent for the data provider. Recommended best practice is to use the GermplasmExperimentID term whenever possible.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/MeasurementExperiment"/>
    <skos:scopeNote rdf:resource="http://terms.gbif.org/wiki/germplasm:measurementExperimentIdentifier"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dcterms:hasVersion rdf:resource="&history;germplasmTerm#measurementExperimentIdentifier"/>
    <vann:termGroup rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmExperimentIdentifier"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">EXPERIMENT_NUMBER</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment number</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <rdfs:domain rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;measurementExperimentYear">
    <skos:prefLabel xml:lang="en">measurement experiment year</skos:prefLabel>
    <skos:definition xml:lang="en">The year when the trait experiment was conducted.</skos:definition>
    <skos:example xml:lang="en">2010</skos:example>
    <skos:note xml:lang="en">For the more detailed experiment date, best practice is to use the dwc:measurementDeterminedDate term.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/MeasurementExperiment"/>
    <skos:scopeNote rdf:resource="http://terms.gbif.org/wiki/germplasm:measurementExperimentYear"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dcterms:hasVersion rdf:resource="&history;germplasmTerm#measurementExperimentYear"/>
    <vann:termGroup rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmExperimentYear"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">EXPERIMENT_YEAR</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment year</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <skos:relatedMatch rdf:resource="dwc:measurementDeterminedDate"/>
    <skos:relatedMatch rdf:resource="dwc:year"/>
    <rdfs:domain rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:range rdf:resource="xsd:integer"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;measurementExperimentReport">
    <skos:prefLabel xml:lang="en">measurement experiment report</skos:prefLabel>
    <skos:definition xml:lang="en">A reference to the report of the experiment, either supplied as the file name or the URL of the report.</skos:definition>
    <skos:example xml:lang="en">URL or reference citation</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to provide the URL to the report.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/MeasurementExperiment"/>
    <skos:scopeNote rdf:resource="http://terms.gbif.org/wiki/germplasm:measurementExperimentReport"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dcterms:hasVersion rdf:resource="&history;germplasmTerm#measurementExperimentReport"/>
    <vann:termGroup rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmExperimentReport"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">EXPERIMENT_REPORT</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment report</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <rdfs:domain rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;measurementExperimentRemarks">
    <skos:prefLabel xml:lang="en">measurement experiment remarks</skos:prefLabel>
    <skos:definition xml:lang="en">Information relevant for the interpretation of the scores in the experiment.</skos:definition>
    <skos:example xml:lang="en">Experimental design, location, experimentor, weather, etc.</skos:example>
    <skos:note xml:lang="en">General remarks related to the experimental conditions or experiment design.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/MeasurementExperiment"/>
    <skos:scopeNote rdf:resource="http://terms.gbif.org/wiki/germplasm:measurementExperimentRemarks"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dcterms:hasVersion rdf:resource="&history;germplasmTerm#measurementExperimentRemarks"/>
    <vann:termGroup rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmExperimentRemarks"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">EXPERIMENT_DESCRIPTION</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment description</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <skos:relatedMatch rdf:resource="dwc:measurementRemarks"/>
    <rdfs:domain rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>


  <!-- Measurement score/value -->

  <skos:Concept rdf:about="&germplasm;measurementByInstituteID">
    <skos:prefLabel xml:lang="en">Trait measurement by institute</skos:prefLabel>
    <skos:definition xml:lang="en">Trait observation determined by institute code, INSTCODE. FAO WIEWS institute code for the institute performing the observation/measurement.</skos:definition>
    <skos:example xml:lang="en">DEU146; NLD037; SWE054; urn:lsid:biocol.org:col:35293</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use institute codes from the FAO WIEWS information system, "http://apps3.fao.org/wiews/institute_query.htm?i_l=EN" or the Biodiversity Collections Index (BCI), "http://www.biodiversitycollectionsindex.org/".</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/MeasurementOrFact"/>
    <skos:scopeNote rdf:resource="http://terms.gbif.org/wiki/germplasm:measurementByInstituteID"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dcterms:hasVersion rdf:resource="&history;germplasmTerm#measurementByInstituteID"/>
    <vann:termGroup rdf:resource="dwc:MeasurementOrFact"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;MeasurementByInstituteCode"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <skos:relatedMatch rdf:resource="dwc:measurementDeterminedBy"/>
    <skos:relatedMatch rdf:resource="dcterms:identifier"/>
    <rdfs:domain rdf:resource="dwc:MeasurementOrFact"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;measurementGrowthStage">
    <skos:prefLabel xml:lang="en">trait measurement at growth stage</skos:prefLabel>
    <skos:definition xml:lang="en">The growth stage of the plant when the trait observation or measurement was made.</skos:definition>
    <skos:example xml:lang="en">flowering; heading; ripening; etc.</skos:example>
    <skos:note xml:lang="en">A more general modifier of the TDWG SDD style could be applied here and include modifiers such as part of the plant where the observation was made (eg. leaf, stem, seeds).</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/MeasurementOrFact"/>
    <skos:scopeNote rdf:resource="http://terms.gbif.org/wiki/germplasm:measurementGrowthStage"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-08-03</dcterms:modified>
    <dcterms:hasVersion rdf:resource="&history;germplasmTerm#measurementGrowthStage"/>
    <vann:termGroup rdf:resource="dwc:MeasurementOrFact"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;MeasurementGrowthStage"/>
    <rdfs:domain rdf:resource="dwc:MeasurementOrFact"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="#anyProperty"/>
  </skos:Concept>


